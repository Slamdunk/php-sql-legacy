{
    "name": "slam/php-sql-legacy",
    "description": "Crappy SQL related classes",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.4.0 || ~8.5.0"
    },
    "require-dev": {
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "phpstan/phpstan": "^2.1.39",
        "phpstan/phpstan-phpunit": "^2.0.16",
        "phpunit/phpunit": "^13.0.5",
        "slam/php-cs-fixer-extensions": "^3.14.0",
        "slam/php-debug-r": "^1.11.0"
    },
    "suggest": {
        "ext-mysqli": "",
        "ext-pdo": "*",
        "ext-pdo_mysql": "",
        "ext-pdo_sqlite": ""
    },
    "autoload": {
        "psr-0": {
            "Db_": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamTest\\Db\\": "tests/"
        }
    }
}
