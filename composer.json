{
    "name": "slam/php-sql-legacy",
    "type": "library",
    "description": "Crappy SQL related classes",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.1.0"
    },
    "require-dev": {
        "malukenho/mcbumpface": "^1.1.5",
        "phpstan/phpstan": "^1.2.0",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "phpunit/phpunit": "^9.5.10",
        "slam/php-cs-fixer-extensions": "^3.1.0",
        "slam/php-debug-r": "^1.7.0"
    },
    "suggest": {
        "ext-PDO": "",
        "ext-mysqli": "",
        "ext-pdo_mysql": "",
        "ext-pdo_sqlite": ""
    },
    "extra": {
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    },
    "autoload": {
        "psr-0": {
            "Db_": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamTest\\Db\\": "tests/"
        }
    }
}
